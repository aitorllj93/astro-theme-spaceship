---
import '@/styles/themes/galaxy-light.css';

import { getStaticPathsForThemePage } from "@/components/Article/utils/get-static-paths";
import Page from '@/components/Article/Page.astro';

const staticPath = await getStaticPathsForThemePage('galaxy');

if (!staticPath) {
  return Astro.redirect('404');
}
---

<Page {...staticPath.props} />